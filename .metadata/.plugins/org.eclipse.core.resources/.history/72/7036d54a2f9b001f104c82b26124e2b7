package practice;

public class Driver6 {
	
	String name;
	int age;
	double salary;
	
	public Driver6(String s,int n) {
		
		name=s;
		age=n;
		
	}
	
	public Driver6(double d) {
		
		salary=d;
	}
	
	public static void main(String[] args) {
		
		Driver6 d1=Driver6("Gani",25);
		Driver6 d2=Driver6(25000);
		
		System.out.println(d1.name);
		System.out.println(d1.age);
		System.out.println(d1.salary);
	}

}
