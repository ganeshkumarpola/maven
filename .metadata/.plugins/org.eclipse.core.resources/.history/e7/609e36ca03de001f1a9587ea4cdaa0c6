package Constructor;

public class Student {
	
	String name;
	int age;
	public Student(String n, int a ) {
		
		name=n;
		age=a;
		
		System.out.println(name+"  :  "+age);
		
	}
	
	public static void main(String[] args) {
		
		Student s=new Student("ganesh",20);
		s.name="ganesh";
		s.age=20;
		
		
	}

}
