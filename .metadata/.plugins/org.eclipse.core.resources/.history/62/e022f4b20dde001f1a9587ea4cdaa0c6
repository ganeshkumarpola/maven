package Abstraction;


abstract class Ganesh2{
	
	public abstract void age();
	public void name() {
		
		System.out.println("Ganesh2 was executed");
	}
}

public class Ganesh1 extends Ganesh2 {
	
	public void age() {
		
		System.out.println("Ganesh1 was executed");
	}
	
	public static void main(String[] args) {
		
		Ganesh1 g=new Ganesh1();
		g.age();
		g.name();
				
				
				
				
				
				
				
				
	
	}
	

}
